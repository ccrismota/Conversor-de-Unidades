<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.cicero.conversordemedias" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ConversorDeMedias" >
        <activity android:name=".ConversorTempo"/>
        <activity android:name=".ConversorVelocidade"/>
        <activity android:name=".ConverterPeso" />
        <activity android:name=".ConversorTemperatura" />
        <activity android:name=".ConversonDados" />
        <activity android:name=".ConversorComprimento" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>